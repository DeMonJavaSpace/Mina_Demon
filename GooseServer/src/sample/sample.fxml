<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="520.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Button fx:id="btnStart" layoutX="187.0" layoutY="12.0" onAction="#startClick" wrapText="true">启动服务器</Button>
        <Button fx:id="btnSend" layoutX="435.0" layoutY="12.0" onAction="#sendClick" wrapText="true">发送报文</Button>
        <Button layoutX="316.0" layoutY="12.0" mnemonicParsing="false" onAction="#closeClick" text="关闭服务器" />
        <ListView fx:id="listView" layoutX="12.0" layoutY="75.0" prefHeight="437.0" prefWidth="777.0" />
        <TextField fx:id="tfPort" layoutX="74.0" layoutY="12.0" prefHeight="23.0" prefWidth="98.0" text="30000" />
        <Label layoutX="30.0" layoutY="16.0" text="端口号" />
      <Label fx:id="tvIp" layoutX="33.0" layoutY="50.0" text="Label" />
      <Label fx:id="tvMac" layoutX="323.0" layoutY="50.0" text="Label" />
    </children>

</AnchorPane>
